<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ada.basic.mapper.basic.BasicDictionaryMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ada.basic.entity.BasicDictionary">
        <id column="id" property="id"/>
        <result column="admin_create" property="adminCreate"/>
        <result column="admin_update" property="adminUpdate"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="code" property="code"/>
        <result column="parent_code" property="parentCode"/>
        <result column="value" property="value"/>
        <result column="name" property="name"/>
        <result column="status" property="status"/>
        <result column="is_delete" property="isDelete"/>
        <result column="description" property="description"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, admin_create, admin_update, create_time, update_time, code, parent_code, value, name, status,is_delete, description
    </sql>



</mapper>
